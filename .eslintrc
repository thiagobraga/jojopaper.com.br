{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["error", {
      "allow": [
        "warn",
        "error"
      ]
    }],
    "no-param-reassign": ["error", {
      "props": false
    }]
  },
  "globals": {
    "$": true
  }
}
